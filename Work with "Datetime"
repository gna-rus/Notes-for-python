from datetime import datetime
from datetime import date
from datetime import timedelta

data1 = "06.11.2021"
dt = datetime.strptime(data1, "%d.%m.%Y") #dateTime
print(dt)
dt = datetime.strptime(data1, "%d.%m.%Y").toordinal() #int
print(dt)
str1 = date.fromordinal(dt).strftime('%d.%m.%Y') # str
print(str1)
dtStr1 = datetime.strptime(str1, '%d.%m.%Y') + timedelta(days=13) # DateTime
print(dtStr1)
print(dtStr1.weekday())
